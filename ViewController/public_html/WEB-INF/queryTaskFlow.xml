<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="queryTaskFlow">
    <?audit suppress oracle.adfdt.controller.adfc.source.audit.TaskFlowReturnActivitiesNotDefined?>
    <default-activity>queryView</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>QueryBean</managed-bean-name>
      <managed-bean-class>com.packt.jdeveloper.cookbook.hr.main.view.beans.QueryBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="queryView">
      <page>/queryView.jsf</page>
    </view>
    <visibility>
      <url-invoke-allowed/>
    </visibility>
  </task-flow-definition>
</adfc-config>
